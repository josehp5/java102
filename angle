public class Point {
    public final double x;
    public final double y;

    public Point(double x, double y) {
        this.x = x;
        this.y = y;
    }

    public double angle() {
        double angleradians = Math.atan2(y, x);
        double angledegrees = Math.todegrees(angleradians);
    }

        return angledegrees;
}
